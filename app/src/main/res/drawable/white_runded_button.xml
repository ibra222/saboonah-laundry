<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
        <item android:state_pressed="true">
              <shape>
                  <solid android:color="#294154"/>
                  <stroke android:color="#454E55" android:width="1dp"/>
                  <corners android:radius="10dp"/>
              </shape>
        </item>

    <item>
        <shape>
            <solid android:color="#213444"/>
            <stroke android:color="#213444" android:width="1dp"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>
</selector>