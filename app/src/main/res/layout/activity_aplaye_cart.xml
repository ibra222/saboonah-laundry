<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                android:layout_height="match_parent"

              >

           <RelativeLayout
               android:id="@+id/toolpar"
               android:layout_width="match_parent"
               android:layout_height="wrap_content">

               <include layout="@layout/snippet_tob_bar"></include>

           </RelativeLayout>
          <RelativeLayout
              android:layout_below="@+id/toolpar"
              android:id="@+id/pross"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:paddingLeft="10dp"
              android:paddingRight="10dp"
              android:paddingTop="10dp"
              android:paddingBottom="10dp">

              <RelativeLayout
                  android:id="@+id/m1"
                  android:layout_marginTop="50dp"
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:layout_margin="15dp">

                  <RelativeLayout
                      android:id="@+id/rels1"
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_marginRight="30dp"
                      android:layout_marginLeft="30dp"
                      android:layout_marginBottom="15dp">

                      <TextView
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:text="وقت الاستلام "
                          android:layout_marginLeft="10dp"
                          android:layout_centerVertical="true"
                          android:fontFamily="@font/jf"
                          android:layout_alignParentRight="true"
                          android:layout_marginRight="25dp"
                          android:textSize="18sp"/>
                      <ImageView
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_centerVertical="true"
                          android:src="@drawable/ic_play_arrow_24px"
                          android:layout_alignParentRight="true"/>
                  </RelativeLayout>

                  <View
                      android:layout_below="@+id/rels1"
                      android:layout_width="match_parent"
                      android:layout_height="1dp"
                      android:background="#A19E9E">

                  </View>

                  <RelativeLayout
                      android:id="@+id/rel2"
                      android:layout_below="@+id/rels1"
                      android:layout_marginTop="10dp"
                      android:layout_marginBottom="10dp"
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content">

                      <TextView
                          android:id="@+id/dateReceipt"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:text="اختار التاريخ "
                          android:fontFamily="@font/jf"
                          android:layout_alignParentRight="true"
                          android:textSize="18sp"
                          />

                      <ImageView
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:src="@drawable/ic_play_arrow_24px"
                          android:layout_centerVertical="true"
                          android:layout_marginRight="60dp"
                          android:layout_toLeftOf="@+id/dateReceipt"/>
                      <View
                          android:layout_width="1dp"
                          android:layout_height="25dp"
                          android:layout_toRightOf="@+id/timeReceipt"
                          android:background="#A19E9E"
                          android:layout_marginLeft="25dp"/>
                      <TextView
                          android:id="@+id/timeReceipt"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:text="اختار الوقت "
                          android:layout_alignParentLeft="true"
                          android:fontFamily="@font/jf"
                          android:layout_marginLeft="75dp"
                          android:textSize="18sp"
                          />

                      <ImageView
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:src="@drawable/ic_play_arrow_24px"
                          android:layout_centerVertical="true"
                          android:layout_alignParentLeft="true"
                          android:layout_marginLeft="15dp"
                         />

                  </RelativeLayout>

                  <View
                      android:layout_below="@+id/rel2"
                      android:layout_width="match_parent"
                      android:layout_height="1dp"
                      android:background="#A19E9E">

                  </View>
              </RelativeLayout>

              <RelativeLayout
                  android:id="@+id/m2"
                  android:layout_below="@+id/m1"
                  android:layout_marginTop="50dp"
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:layout_margin="10dp"
                  >

                  <RelativeLayout
                      android:id="@+id/rels13"
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_marginRight="30dp"
                      android:layout_marginLeft="30dp"
                      android:layout_marginBottom="15dp">

                      <TextView
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:text="وقت الاستلام "
                          android:layout_marginLeft="10dp"
                          android:layout_centerVertical="true"
                          android:layout_alignParentRight="true"
                          android:layout_marginRight="25dp"
                          android:fontFamily="@font/jf"
                          android:textSize="18sp"
                          />

                      <ImageView
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_centerVertical="true"
                          android:src="@drawable/ic_play_arrow_24px"
                          android:layout_alignParentRight="true"/>
                  </RelativeLayout>

                  <View
                      android:layout_below="@+id/rels13"
                      android:layout_width="match_parent"
                      android:layout_height="1dp"
                      android:background="#A19E9E">

                  </View>

                  <RelativeLayout
                      android:id="@+id/rel22"
                      android:layout_below="@+id/rels13"
                      android:layout_marginTop="10dp"
                      android:layout_marginBottom="10dp"
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content">

                      <TextView
                          android:id="@+id/dateDelivery"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:text="اختار التاريخ "
                          android:layout_alignParentRight="true"
                          android:textSize="18sp"
                          android:fontFamily="@font/jf"
                          />

                      <ImageView
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:src="@drawable/ic_play_arrow_24px"
                          android:layout_centerVertical="true"
                          android:layout_marginRight="60dp"
                          android:layout_toLeftOf="@+id/dateDelivery"
                          />
                      <View
                          android:layout_width="1dp"
                          android:layout_height="25dp"
                          android:layout_toRightOf="@+id/timeDelivery"
                          android:background="#A19E9E"
                          android:layout_marginLeft="25dp"/>
                      <TextView
                          android:id="@+id/timeDelivery"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:text="اختار الوقت "
                          android:fontFamily="@font/jf"
                          android:layout_alignParentLeft="true"
                          android:layout_marginLeft="75dp"
                          android:textSize="18sp"
                          />

                      <ImageView
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:src="@drawable/ic_play_arrow_24px"
                          android:layout_centerVertical="true"
                          android:layout_alignParentLeft="true"
                          android:layout_marginLeft="15dp"
                          />
                  </RelativeLayout>

                  <View
                      android:layout_below="@+id/rel22"
                      android:layout_width="match_parent"
                      android:layout_height="1dp"
                      android:background="#A19E9E">

                  </View>
              </RelativeLayout>

              <RelativeLayout
                  android:id="@+id/m4"
                  android:layout_below="@+id/m2"
                  android:layout_marginTop="10dp"
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content">

                  <com.google.android.gms.maps.MapView
                      android:id="@+id/mapView"
                      android:layout_width="match_parent"
                      android:layout_height="100dp"
                      android:layout_centerHorizontal="true"
                      />

              </RelativeLayout>

              <RelativeLayout
                  android:id="@id/m5"
                  android:layout_below="@id/m4"
                  android:layout_marginTop="11sp"
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content">

                  <RelativeLayout
                      android:id="@+id/rels16"
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_marginRight="30dp"
                      android:layout_marginLeft="30dp"
                      android:layout_marginBottom="15dp">

                      <TextView
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:text="وقت الاستلام "
                          android:layout_marginLeft="10dp"
                          android:fontFamily="@font/jf"
                          android:layout_centerVertical="true"
                          android:layout_alignParentRight="true"
                          android:layout_marginRight="25dp"
                          android:textSize="18sp"
                          />

                      <ImageView
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_centerVertical="true"
                          android:src="@drawable/ic_play_arrow_24px"
                          android:layout_alignParentRight="true"/>

                  </RelativeLayout>

                  <EditText
                      android:layout_below="@+id/rels16"
                      android:id="@+id/subaddress"
                      android:layout_width="match_parent"
                      android:layout_height="60dp"
                      android:background="@color/main"
                      android:textColor="@color/white"
                      android:hint="النصر -برج ابو سمرة -الطابق الخامس"
                      android:textColorHint="@color/white"
                      android:textDirection="rtl"
                      android:fontFamily="@font/jf"
                      android:inputType="text|textMultiLine"
                      android:gravity="center_vertical"
                      />

              </RelativeLayout>


              <RelativeLayout
                  android:id="@+id/m6"
                  android:layout_below="@+id/m5"
                  android:layout_marginTop="15dp"
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content">

                  <LinearLayout
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_marginLeft="15dp"
                      android:layout_marginRight="15dp"
                      android:orientation="horizontal">

                      <Button
                          android:id="@+id/edit"
                          android:layout_width="133dp"
                          android:layout_height="40dp"
                          android:background="@drawable/white_runded_button"
                          android:text="التعديل بالخلف"
                          android:layout_gravity="left"
                          android:fontFamily="@font/jf"
                          android:layout_marginLeft="25dp"

                          android:textColor="@color/white"/>

                      <Button
                          android:id="@+id/aplaye"
                          android:layout_width="133dp"
                          android:layout_height="40dp"
                          android:background="@drawable/white_runded_button"
                          android:text="اعتماد وتتبع الطلبية"
                          android:textColor="@color/white"
                          android:fontFamily="@font/jf"
                          android:layout_marginLeft="10dp"
                          />


                  </LinearLayout>

              </RelativeLayout>



          </RelativeLayout>


</RelativeLayout>